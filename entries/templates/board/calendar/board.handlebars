
<div id="calendar">
  <div class="page-header">
    <div class="form-inline">
      <div class="btn-group">
        <button class="btn btn-primary" data-calendar-nav="prev"><< 向前</button>
        <button class="btn" data-calendar-nav="today">今天</button>
        <button class="btn btn-primary" data-calendar-nav="next">向后 >></button>
      </div>
      <div class="btn-group">
        <button class="btn btn-warning" data-calendar-view="year">按年</button>
        <button class="btn btn-warning active" data-calendar-view="month">按月</button>
        <button class="btn btn-warning" data-calendar-view="week">按周</button>
        <button class="btn btn-warning" data-calendar-view="day">按天</button>
      </div>
    </div>
  </div>
  <div class="content"></div>
</div>